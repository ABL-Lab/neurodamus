{
    "version": 1,
    "manifest": {
        "$OUTPUT_DIR": "./reporting",
        "$INPUT_DIR": "./"
    },
    "node_sets_file": "nodesets.json",
    "node_set": "l4mc",
    "run": {
        "tstart": 0,
        "tstop": 1.0,
        "dt": 0.1,
        "seed": 1122
    },
    "output": {
        "output_dir": "$OUTPUT_DIR",
        "spikes_file": "spikes.h5",
        "spikes_sort_order": "time"
    },
    "reports": {
        "soma_report": {
            "cells": "l4mc",
            "variable_name": "v",
            "sections": "soma"
        },
        "compartment_report": {
            "variable_name": "v",
            "sections": "all"
        }
    }
}
